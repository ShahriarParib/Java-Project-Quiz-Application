<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #F3CC79;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizapplication.Quiztreeview">
    <children>
        <!-- Back Button -->
        <Button fx:id="backButton" alignment="CENTER" layoutX="-21.0" layoutY="-2.0" mnemonicParsing="false" onAction="#handleback" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: #2B2B52;" text="   Back" textFill="#FAF8F8" />

        <!-- ScrollPane with TreeView -->
        <ScrollPane layoutY="30.0" prefHeight="738.0" prefWidth="1024.0" style="-fx-background-color: transparent;">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="718.0" prefWidth="1024.0" style="-fx-background-color: transparent;">
                    <children>
                        <TreeView fx:id="treeview" layoutY="0.0" prefHeight="718.0" prefWidth="1024.0" style="-fx-background-color: transparent;" />
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
